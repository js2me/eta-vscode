{
	"name": "Eta",
	"patterns": [
		{
			"include": "#tag-eta-single-line"
		}
	],
	"injectionSelector": "L:text.html",
	"repository": {
		"tag-eta-single-line": {
			"name": "keyword.control",
			"begin": "(<%[-_]?\\s*?(~|=)?)",
			"end": "([-_]?%>)",
			"contentName": "meta.embedded.block.javascript",
			"patterns": [
				{
					"include": "source.js"
				}
			]
		}
	},
	"scopeName": "text.html.eta"
}